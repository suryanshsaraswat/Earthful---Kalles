      "blocks": {
        "68ba7ab1-9829-4e77-9f95-d34b3eb96aba": {
          "type": "image",
          "settings": {
            "mul_cap": "shopify:\/\/shop_images\/Ajwain_Capsule_2cd5724b-e2d4-4f7d-8605-f7422570c4c5.png",
